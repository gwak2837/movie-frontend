{
  "name": "movie-frontend",
  "version": "1.0.0",
  "description": "Movie Frontend with Next.js",
  "author": "Taeuk Gwak",
  "license": "GPL-3.0-only",
  "repository": "https://github.com/rmfpdlxmtidl/movie-frontend.git",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "predev": "yarn generate",
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "generate": "graphql-codegen --config codegen.yml && prettier src/graphql/generated/*.* --write",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@hookform/error-message": "^0.0.5",
    "antd": "^4.9.4",
    "graphql": "^15.4.0",
    "inko": "^1.1.1",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^2.2.0",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn tsc"
    }
  }
}
